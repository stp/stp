% RUN: %solver %s | %OutputCheck %s
%% Regression level = 3
%% Result = InValid
%% Language = presentation

n: BITVECTOR(32);
c: BITVECTOR(8);
%----------------------------------------------------
ASSERT BVLT(n,0bin00000000000000000000000000010000);
ASSERT NOT ((0bin000000000000000000000000 @ c) 
     = 0bin00000000000000000000000011111110);
ASSERT NOT BVLT(n,0bin00000000000000000000000000001100);
ASSERT NOT ((0bin000000000000000000000000 @ ((0bin000000000000000000000000 
                                       @ c))
      [7:0]) = 0bin00000000000000000000000000000000);
ASSERT NOT NOT ((n & 0bin00000000000000000000000000000011) 
         = 0bin00000000000000000000000000000000);

%----------------------------------------------------
% CHECK: ^[Ii]nvalid
QUERY FALSE;
COUNTEREXAMPLE;
